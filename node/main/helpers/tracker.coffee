exports.servePixel = (res) ->
	buffer = new Buffer [0x47, 0x49, 0x46, 0x38, 0x39, 0x61, 0x01, 0x00, 0x01, 0x00, 0x80, 0x00, 0x00, 0xff, 0xff, 0xff, 0x00, 0x00, 0x00, 0x2c, 0x00, 0x00, 0x00, 0x00, 0x01, 0x00, 0x01, 0x00, 0x00, 0x02, 0x02, 0x44, 0x01, 0x00, 0x3b]

	res.set
		'Content-Type': 'image/gif'
		'Cache-Control': 'private, no-cache, no-cache=Set-Cookie, proxy-revalidate',
		'Expires': new Date(0).toUTCString()
	
	res.send buffer, 200